   "source": [
    "# Using factorial from my_funcs.py\n",
    "\n",
    "import myfuncs\n",
    "\n",
    "n = int(input('Enter number: '))\n",
    "fact = myfuncs.factorial(n)\n",
    "\n",
    "for i in range(1, n + 1):\n",
    "    print(i, end=' ')\n",
    "    if i != n:\n",
    "      print('*', end=' ')\n",
    "\n",
    "print('=', fact)"
